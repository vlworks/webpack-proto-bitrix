{
  "compilerOptions": {
    "outDir": "./bundle/",
    "allowJs": true,
    "module": "es6",
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "downlevelIteration": true,
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": false,
    "moduleResolution": "node",
  },
}
